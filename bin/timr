#!/usr/bin/env ruby
# coding: UTF-8

require 'timr'

START_TIME = Time.now.to_ms
# puts "#{START_TIME} #{self.class} #{__method__}"

command = TheFox::Timr::Command.create_command_from_argv(ARGV.clone)
command.run
command.shutdown

END_TIME = Time.now.to_ms
# puts "#{END_TIME} #{self.class} #{__method__}"
# puts "runtime: #{END_TIME - START_TIME} ms"
