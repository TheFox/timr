#!/usr/bin/env ruby
# coding: UTF-8

require 'timr'

include TheFox::Timr::Command

START_TIME = Time.now.to_ms
puts "#{START_TIME} #{self.class} #{__method__}"

command = Command.create_command_from_argv(ARGV.clone)
begin
	command.run
rescue TheFox::Timr::TimrError => e
	puts "error: #{e}"
end
command.shutdown

END_TIME = Time.now.to_ms
puts "#{END_TIME} #{self.class} #{__method__}"
puts "runtime: #{END_TIME - START_TIME} ms"
